#include<bits/stdc++.h>
#include<iostream>
using namespace std;


int main(){
	int n,q;
	string s;
	cin>>n>>s>>q;
	int t;
	int a,b;
	for(int i=0;i<q;i++){
		cin>>t>>a>>b;

		bool flag = false;
		if(t==2){
			flag++;
		}else{
			if(flag==false){
				swap(s[a-1], s[b-1]);
			}else{
				a = (a+n)%n;
				b = (b+n)%n;
				swap(s[a-1], s[b-1]);
			}
		}
//	cout<<s1<<s2<<endl;
	}
	cout<<s1<<s2<<endl;



	return 0;
}
